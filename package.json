{
  "name": "@tinyhttp/swagger",
  "version": "0.1.2",
  "description": "Swagger integration for tinyhttp",
  "author": "massivefermion <massivefermion@protonmail.com>",
  "keywords": [
    "swagger",
    "openapi",
    "tinyhttp",
    "documentation"
  ],
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinyhttp/swagger"
  },
  "bugs": {
    "url": "https://github.com/tinyhttp/swagger/issues"
  },
  "homepage": "https://github.com/tinyhttp/swagger#readme",
  "engines": {
    "node": "12.x || 14.x || >=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "NODE_OPTIONS='--loader tsx' uvu tests",
    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "lint": "eslint \"./**/*.ts\"",
    "format": "prettier --check \"./**/*.ts\"",
    "format:fix": "prettier --write \"./**/*.ts\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "c8": "^7.13.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "rollup": "^3.19.1",
    "rollup-plugin-copy": "^3.4.0",
    "supertest-fetch": "^1.5.0",
    "tslib": "^2.5.0",
    "tsx": "^3.12.5",
    "typescript": "^5.0.2",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "@tinyhttp/app": "^2.0.29"
  }
}
