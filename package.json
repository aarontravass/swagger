{
  "name": "swagger",
  "version": "0.0.0",
  "description": "Swagger integration for Tinyhttp",
  "author": "massivefermion <massivefermion@protonmail.com>",
  "keywords": ["swagger", "tinyhttp", "documentation"],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinyhttp/swagger"
  },
  "bugs": {
    "url": "https://github.com/tinyhttp/swagger/issues"
  },
  "homepage": "https://github.com/tinyhttp/swagger#readme",
  "directories": {
    "lib": "./src",
    "example": "./example",
    "test": "./test"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@tinyhttp/app": "^1.3.11",
    "@tinyhttp/router": "^1.3.3",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "c8": "^7.7.3",
    "esbuild-node-loader": "^0.1.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.2",
    "rollup": "^2.52.2",
    "rollup-plugin-copy": "^3.4.0",
    "supertest-fetch": "^1.4.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2",
    "uvu": "^0.5.1"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --ignore-path .gitignore --ext=js,mjs,ts,cjs",
    "format": "prettier --check --ignore-path .gitignore \"./**/*.{ts,js,mjs,cjs,md}\"",
    "format:fix": "prettier --write --ignore-path .gitignore \"./**/*.{ts,js,mjs,cjs,md}\"",
    "coverage": "c8 --reporter=lcov --reporter=text pnpm test",
    "test": "node --experimental-loader esbuild-node-loader node_modules/uvu/bin.js tests",
    "prepare": "husky install"
  }
}